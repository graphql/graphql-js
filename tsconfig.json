{
  "exclude": ["integrationTests/ts/**/*"],
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es2018"],
    "target": "ES2018",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "types": [],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  }
}
