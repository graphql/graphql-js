// @flow
export type Path = {|
  +prev: Path | void,
  +key: string | number,
  +typename: string | void,
|};

/**
 * Given a Path and a key, return a new Path containing the new key.
 */
declare export function addPath(
  prev: $ReadOnly<Path> | void,
  key: string | number,
  typename: string | void,
): Path;

/**
 * Given a Path, return an Array of the path keys.
 */
declare export function pathToArray(
  path: ?$ReadOnly<Path>,
): Array<string | number>;
