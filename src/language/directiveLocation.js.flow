  MUTATION: string,
  SUBSCRIPTION: string,
  FIELD: string,
  FRAGMENT_DEFINITION: string,
  FRAGMENT_SPREAD: string,
  INLINE_FRAGMENT: string,
  VARIABLE_DEFINITION: string,
  SCHEMA: string,
  SCALAR: string,
  OBJECT: string,
  FIELD_DEFINITION: string,
  ARGUMENT_DEFINITION: string,
  INTERFACE: string,
  UNION: string,
  ENUM: string,
